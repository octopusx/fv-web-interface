<?xml version="1.0" encoding="ISO-8859-1"?>
<profile>
	<slice name="general_traffic">
		<controller_url>tcp:127.0.0.1:6634</controller_url>
		<admin_email>octopusx@o2.pl</admin_email>
		<password>password</password>
		<drop_policy>exact</drop_policy>
		<receive_lldp>true</receive_lldp>
		<flowmod_limit>-1</flowmod_limit>
		<rate_limit>1000000000</rate_limit>
		<admin_status>true</admin_status>
		
		<flowspace name="ip">
			<dpid>00:00:00:00:00:00:00:01</dpid>
			<priority>1</priority>
			<match>
				<rule>
					<name>dl_type</name>
					<value>2048</value>
				</rule>
			</match>
			<queues>
				<id>0</id>
				<id>1</id>
			</queues>
			<force_enqueue>0</force_enqueue>
			<permission>7</permission>
		</flowspace>
		<flowspace name="arp">
			<dpid>00:00:00:00:00:00:00:01</dpid>
			<priority>1</priority>
			<match>
				<rule>
					<name>dl_type</name>
					<value>2054</value>
				</rule>
			</match>
			<queues>
				<id>0</id>
				<id>1</id>
			</queues>
			<force_enqueue>0</force_enqueue>
			<permission>7</permission>
		</flowspace>
		<flowspace name="rarp">
			<dpid>00:00:00:00:00:00:00:01</dpid>
			<priority>1</priority>
			<match>
				<rule>
					<name>dl_type</name>
					<value>32821</value>
				</rule>
			</match>
			<queues>
				<id>0</id>
				<id>1</id>
			</queues>
			<force_enqueue>0</force_enqueue>
			<permission>7</permission>
		</flowspace>
	</slice>	
	<config>
		<flood_perm>slice1</flood_perm>
		<flowmod_limit>
			<rule>
				<slice>slice1</slice>
				<dpid>00:00:00:00:00:00:00:01</dpid>
				<limit>-1</limit>
			</rule>
		</flowmod_limit>
		<track_flows>true</track_flows>
		<stats_desc>true</stats_desc>
		<enable_topo_ctrl>false</enable_topo_ctrl>
		<flow_stats_cache>30</flow_stats_cache>
	</config>
</profile>
